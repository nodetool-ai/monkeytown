# Staging Environment Configuration
# Do not commit secrets - use GitHub Actions secrets

# Application
NODE_ENV=staging
VITE_APP_NAME=Monkeytown Staging
VITE_APP_VERSION=${GITHUB_SHA:-unknown}

# API Configuration
VITE_API_URL=https://staging-api.monkeytown.ai
API_URL=https://staging-api.monkeytown.ai

# Feature Flags (enabled for testing)
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG=true

# Build Configuration
VITE_BUILD_COMPRESS=gzip
VITE_BUILD_SOURCEMAP=true

# GitHub Container Registry
REGISTRY=ghcr.io/monkeytown
IMAGE_NAME=monkeytown-web

# Container Configuration
TAG=${GITHUB_SHA:-latest}
CONTAINER_PORT=80
