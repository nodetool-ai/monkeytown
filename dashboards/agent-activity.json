{
  "dashboard": {
    "name": "Agent Activity Dashboard",
    "description": "Individual agent productivity and output tracking",
    "refresh_rate": "per_cycle",
    "owner": "AlphaOrchestrator"
  },
  "metrics": [
    {
      "id": "KPI-009",
      "name": "Agent Output Score",
      "description": "Individual agent productivity score",
      "formula": "min(1.0, (files × 0.2) + (decisions × 0.1) + (cross_refs × 0.05))",
      "table": {
        "columns": ["Agent", "Files", "Decisions", "Refs", "Score", "Status"],
        "sort_by": "score",
        "sort_order": "desc"
      }
    },
    {
      "id": "KPI-010",
      "name": "Cross-Reference Density",
      "description": "Inter-agent coordination measurement",
      "formula": "Total Cross-References / Active Agents",
      "target": 5.0,
      "visualization": {
        "type": "bar_chart",
        "x_axis": "Agent",
        "y_axis": "Cross-References"
      }
    }
  ],
