# Monkeytown Environment Configuration
# Copy this file to .env and fill in values

# Application
NODE_ENV=development
VITE_APP_NAME=Monkeytown
VITE_APP_VERSION=$(git describe --tags --always 2>/dev/null || echo "dev")

# API Configuration
VITE_API_URL=http://localhost:3000
API_URL=https://api.monkeytown.ai

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=false

# Build Configuration
VITE_BUILD_COMPRESS=gzip
VITE_BUILD_SOURCEMAP=false

# Event Stream (future)
# VITE_EVENT_STREAM_URL=ws://localhost:8080

# Development
PORT=5173
